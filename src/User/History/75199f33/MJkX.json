{
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.vimrc.enable": true,
  "editor.fontFamily": "Menlo, monospace",
  "editor.fontWeight": 500,
  "editor.fontSize": 12,
  "terminal.integrated.fontFamily": "monospace",
  "editor.minimap.enabled": false,
  "workbench.settings.editor": "ui",
  "gruvboxMaterial.darkContrast": "hard",
  "gruvboxMaterial.colorfulSyntax": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.automationShell.osx": "/bin/bash",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    }
  },
  "editor.tabSize": 2,
  "gruvboxMaterial.darkPalette": "mix",
  "explorer.confirmDelete": false,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "vim.easymotionDimBackground": false,
  "editor.renderLineHighlight": "none",
  "editor.tabCompletion": "on",
  "prettier.arrowParens": "avoid",
  "prettier.endOfLine": "auto",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "rewrap.wrappingColumn": 70,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "editor.formatOnSave": true,
  "gruvboxMaterial.lightContrast": "hard",
  "workbench.colorTheme": "Vim Light Hard",
  "workbench.activityBar.visible": false,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.hidesExplorerArrows": true,
  "files.exclude": {
    "**/node_modules": true
  },
  "workbench.tree.indent": 16,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "window.zoomLevel": 1
}
